# Laravel API (customer/on-demand endpoints)
LARAVEL_API_BASE_URL=http://localhost:8000/api

# OpenAI (for LangChain/LangGraph LLM)
OPENAI_API_KEY=

# LangSmith (tracing: agent, human messages, tool calls)
# Required for runs to show data at https://smith.langchain.com
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=
# Optional: LANGCHAIN_PROJECT=fixerity-agent

# Mem0 Platform (memory for buyer/seller agents)
MEM0_API_KEY=

# Agent service port
PORT=3001
# or: AGENT_SERVICE_PORT=3001
